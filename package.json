{
  "name": "inklearn-hub",
  "version": "1.0.0",
  "description": "InkLearn-Hub is a dynamic and user-friendly application designed to streamline the educational journey for both teachers and students. This innovative platform serves as a centralized hub for managing academic processes and lectures with utmost efficiency. Teachers can seamlessly organize and administer their courses, while students benefit from a structured learning experience. With its intuitive features, InkLearn-Hub fosters collaboration, communication, and a well-organized educational environment, enhancing the overall teaching and learning experience for all users.",
  "main": "./out/main/index.js",
  "author": "saihanhtet",
  "homepage": "https://saihanhtet.vercel.app/",
  "scripts": {
    "format": "prettier --write .",
    "lint": "eslint . --ext .js,.jsx,.cjs,.mjs,.ts,.tsx,.cts,.mts --fix",
    "typecheck:node": "tsc --noEmit -p tsconfig.node.json --composite false",
    "typecheck:web": "tsc --noEmit -p tsconfig.web.json --composite false",
    "typecheck": "npm run typecheck:node && npm run typecheck:web",
    "start": "electron-vite preview",
    "dev": "electron-vite dev",
    "build": "npm run typecheck && electron-vite build",
    "postinstall": "electron-builder install-app-deps",
    "build:win": "npm run build && electron-builder --win --config",
    "build:mac": "electron-vite build && electron-builder --mac --config",
    "build:linux": "electron-vite build && electron-builder --linux --config"
  },
  "dependencies": {
    "@electron-toolkit/preload": "^3.0.0",
    "@electron-toolkit/utils": "^3.0.0",
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.11",
    "@mui/x-charts": "^6.19.5",
    "axios": "^1.6.5",
    "bootstrap": "^5.3.2",
    "bulma-toast": "^2.4.3",
    "electron-updater": "^6.1.7",
    "react-bootstrap": "^2.7.0",
    "react-icons": "^5.0.1",
    "react-router-dom": "^6.21.3",
    "tree-kill": "^1.2.2"
  },
  "devDependencies": {
    "@electron-toolkit/eslint-config-prettier": "^2.0.0",
    "@electron-toolkit/eslint-config-ts": "^1.0.1",
    "@electron-toolkit/tsconfig": "^1.0.1",
    "@types/node": "^18.19.5",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react": "^4.2.1",
    "electron": "^28.1.1",
    "electron-builder": "^24.9.1",
    "electron-vite": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-plugin-react": "^7.33.2",
    "prettier": "^3.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.3",
    "vite": "^5.0.11"
  }
}
